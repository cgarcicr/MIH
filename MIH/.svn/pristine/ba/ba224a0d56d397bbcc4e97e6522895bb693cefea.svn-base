/* BOTONES
----------------------------------------------------------*/
.boton
{
    background: #ff00ff;
    background: -moz-linear-gradient(top, #ff00ff, #ff00ff);
    background: #ff00ff; /* Borders */
    -moz-box-shadow: inset 0px 0px 1px #edf9ff;
    -webkit-box-shadow: inset 0px 0px 1px #edf9ff;
    box-shadow: inset 0px 0px 1px #edf9ff; /*filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#0056B2', endColorstr='#09488B');*/
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    border: 1px solid #ff00ff;
    display: inline-block;
    *display: inline;
	zoom: 1;
	
    color: #ffffff;
    font-family: arial;
    font-size: 10px;
    font-weight: normal;
    text-decoration: none;
    text-shadow: 1px 1px 0px #1570cd;
    padding: 4px 15px;
    margin-left: 0px;
    margin-top: 0px;
    margin-bottom: 0px;
}
.boton input:hover
{
    background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #F4F4F4), color-stop(1, #EEEEEE) );
    background: -moz-linear-gradient( center top, #F4F4F4 5%, #EEEEEE 100% );
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F4F4F4', endColorstr='#EEEEEE');
    background-color: #ff0080;
    color: #161616;
    text-shadow: 1px 1px 1px #FFFFFF;
}
.boton input:active
{
    position: relative;
    top: 1px;
}

.boton input:disabled
{
    -moz-box-shadow: inset 0px 1px 0px 0px #ffffff;
    -webkit-box-shadow: inset 0px 1px 0px 0px #ffffff;
    box-shadow: inset 0px 1px 0px 0px #ffffff;
    background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #ededed), color-stop(1, #dfdfdf) );
    background: -moz-linear-gradient( center top, #ededed 5%, #dfdfdf 100% );
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#dfdfdf');
    background-color: #ededed;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    border: 1px solid #dcdcdc;
    display: inline-block;
    *display: inline;
	zoom: 1;
	
    color: #777777;
    font-family: arial;
    font-size: 15px;
    font-weight: bold;
    padding: 6px 24px;
    text-decoration: none;
    text-shadow: 1px 1px 0px #ffffff;
}
